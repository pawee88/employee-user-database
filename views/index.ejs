<!-- Include Header -->
<%-include('include/_header')%>
<!-- /Include Header -->

<!-- Main Site -->
<main id="site-main">
  <div class="container">
    <div class="animation">
      <lottie-player
        src="https://assets5.lottiefiles.com/packages/lf20_ymbzgxgc.json"
        background="transparent"
        speed="1"
        style="width: 200px; height: 200px"
        loop
        autoplay
      ></lottie-player>
    </div>
    <div class="box-nav d-flex justify-between">
      <a href="/add-user" class="border-shadow">
        <span class="text-gradient">New user <i class="fas fa-user"></i></span
      ></a>
    </div>

    <!-- form Handling -->
    <form action="/" method="POST">
      <table class="table">
        <thead class="thead-dark">
          <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Email</th>
            <th>Phone</th>
            <th>Position</th>
            <th>Gender</th>
            <th>Status</th>
            <th>Action</th>
          </tr>
        </thead>
        <tbody>
          <%-include('include/_show')%>
        </tbody>
      </table>
    </form>
  </div>
</main>

<!-- /Main Site -->

<!-- Include footer -->
<%-include('include/_footer')%>
<!-- /Include footer -->
`
